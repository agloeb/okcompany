{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "pages/api/submit-order.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)/?",
      "dest": "/public/$1.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}