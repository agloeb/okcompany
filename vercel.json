{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "pages/api/submit-order.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-order",
      "dest": "/pages/api/submit-order.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
